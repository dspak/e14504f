}

@online{michael_edidin_state_2003,
	title = {{THE} {STATE} {OF} {LIPID} {RAFTS}: From Model Membranes to Cells},
	rights = {Copyright c 2003 by Annual Reviews. All rights reserved},
	url = {http://arjournals.annualreviews.org/doi/full/10.1146/annurev.biophys.32.110601.142439},
	shorttitle = {{THE} {STATE} {OF} {LIPID} {RAFTS}},
	type = {review-article},
	author = {Michael Edidin},
	urldate = {2008-10-01},
	date = {2003-11-28},
	note = {▪ Abstract Lipid raft microdomains were conceived as part of a mechanism for the intracellular trafficking of lipids and lipid-anchored proteins. The raft hypothesis is based on the behavior of defined lipid mixtures in liposomes and other model membranes. Experiments in these well-characterized systems led to operational definitions for lipid rafts in cell membranes. These definitions, detergent solubility to define components of rafts, and sensitivity to cholesterol deprivation to define raft functions implicated sphingolipid- and cholesterol-rich lipid rafts in many cell functions. Despite extensive work, the basis for raft formation in cell membranes and the size of rafts and their stability are all uncertain. Recent work converges on very small rafts {\textless}10 nm in diameter that may enlarge and stabilize when their constituents are cross-linked.},
	keywords = {sphingolipids; cholesterol; membrane domains; fluidity; trafficking},
	file = {THE STATE OF LIPID RAFTS\: From Model Membranes to Cells - Annual Review of Biophysics and Biomolecular Structure, 32(1)\:257 - Full Text:/Users/danielspakowicz/Zotero/storage/GEFHTBGA/annurevbiophys32110601.html:text/html}
}

