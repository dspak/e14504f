	volume = {106},
	issn = {1097-0290},
	doi = {10.1002/bit.22668},
	abstract = {The mevalonate pathway in the yeast Saccharomyces cerevisiae was deregulated in order to enhance the intracellular pool of farnesyl diphosphate ({FPP}), the direct precursor for the biosynthesis of sesquiterpenes. Over-expression of the catalytic domain of {HMG}1, both from the genome and plasmid, resulted in higher production of cubebol, a plant originating sesquiterpene, and increased squalene accumulation. Down-regulation of {ERG}9 by replacing its native promoter with the regulatable {MET}3 promoter, enhanced cubebol titers but simultaneous over-expression of {tHMG}1 and repression of {ERG}9 did not further improve cubebol production. Furtheremore, the concentrations of squalene and ergosterol were measured in the engineered strains. Unexpectedly, significant accumulation of squalene and restoring the ergosterol biosynthesis were observed in the {ERG}9 repressed strains transformed with the plasmids harboring cubebol synthase gene. This could be explained by a toxicity effect of cubebol, possibly resulting in higher transcription levels for the genes under control of {MET}3 promoter, which could lead to accumulation of squalene and ergosterol.},
	pages = {86--96},
	number = {1},
	journaltitle = {Biotechnology and bioengineering},
	shortjournal = {Biotechnol. Bioeng.},
	author = {Asadollahi, Mohammad A and Maury, Jérôme and Schalk, Michel and Clark, Anthony and Nielsen, Jens},
	date = {2010-05-01},
	pmid = {20091767},
	keywords = {Farnesyl-Diphosphate Farnesyltransferase, Gene Expression Regulation, Fungal, Genetic engineering, {HMGB}1 Protein, Metabolic Networks and Pathways, Mevalonic Acid, Plasmids, Polyisoprenyl Phosphates, Promoter Regions, Genetic, Saccharomyces cerevisiae, Saccharomyces cerevisiae Proteins, Sesquiterpenes}
}

@misc{supelco_bulletin_869a_solid_????,
	title = {Solid Phase Microextraction: Solventless Sample Preparation for Monitoring Flavor Compounds by Capillary Gas Chromatography},
	url = {www.sigmaaldrich.com/etc/medialib/docs/Supelco/.../4524.pdf},
	abstract = {Solid phase microextraction is a fast, solventless alternative to conventional sample extraction techniques. In {SPME}, analytes establish equilibria among the sample matrix, the headspace above the sample, and a stationary phase coated on a fused silica fiber, then are thermally desorbed from the fiber to a capillary {GC} column. Because no solvent is injected, and the analytes are rapidly desorbed onto the column, minimum detection limits are improved and reso- lution is maintained. {SPME} is useful in many diverse analy- ses, including characterization of flavor components in foods and beverages and fragrance compounds in a wide range of products.},
