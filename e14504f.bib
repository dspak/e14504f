@article{bok_genomic_2006,
	title = {Genomic Mining for Aspergillus Natural Products},
	volume = {13},
	url = {http://www.sciencedirect.com/science/article/B6VRP-4J31WV6-7/2/0af8a5d4c18192b1b05eada14f139fa3},
	doi = {10.1016/j.chembiol.2005.10.008},
	abstract = {Summary The genus Aspergillus is renowned for its ability to produce a myriad of bioactive secondary metabolites. Although the propensity of biosynthetic genes to form contiguous clusters greatly facilitates assignment of putative secondary metabolite genes in the completed Aspergillus genomes, such analysis cannot predict gene expression and, ultimately, product formation. To circumvent this deficiency, we have examined Aspergillus nidulans microarrays for expressed secondary metabolite gene clusters by using the transcriptional regulator {LaeA}. Deletion or overexpression of {laeA} clearly identified numerous secondary metabolite clusters. A gene deletion in one of the clusters eliminated the production of the antitumor compound terrequinone A, a metabolite not described, from A. nidulans. In this paper, we highlight that {LaeA}-based genome mining helps decipher the secondary metabolome of Aspergilli and provides an unparalleled view to assess secondary metabolism gene regulation.},
	pages = {31--37},
	number = {1},
	journaltitle = {Chemistry \& Biology},
	author = {Bok, Jin Woo and Hoffmeister, Dirk and Maggio-Hall, Lori A. and Murillo, Renato and Glasner, Jeremy D. and Keller, Nancy P.},
	urldate = {2008-10-21},
	date = {2006-01},
	file = {Google Scholar Linked Page:/Users/danielspakowicz/Zotero/storage/P9TX9GAI/S1074552105003455.html:text/html;ScienceDirect Full Text PDF:/Users/danielspakowicz/Zotero/storage/7DUEIUMD/Bok et al - 2006 - Genomic Mining for Aspergillus Natural Products.pdf:application/pdf;ScienceDirect Snapshot:/Users/danielspakowicz/Zotero/storage/IBEUBJ2Z/30e293746f0e641d10c1a00942031764.html:text/html}
}

@article{cook_mentha_2007,
	title = {Mentha Spicata Essential Oils Rich In 1,8-Cineole And 1,2-Epoxy-P-Menthane Derivatives From Zakynthos (Ionian Island, W Greece)},
	volume = {19},
	issn = {1041-2905},
	url = {http://www.tandfonline.com/doi/abs/10.1080/10412905.2007.9699266},
	doi = {10.1080/10412905.2007.9699266},
	abstract = {Abstract Wild Mentha spicata plants from 3 locations on the Island of Zakynthos had essential oil yields ({mL}/100g {DW}) ranging from 0.5–1.1 \%. The main oil constituents were trans-piperitone oxide, piperitenone oxide and 1,8-cineole. On a whole plant basis (aerial parts) the trans-piperitone oxide content ranged from 1.4 \% location (Loc 1) to 32.5\% (Loc 3) and appeared to have an inverse relationship with the 1,8-cineole content which ranged from 10.8 \% (Loc 3) to 37.9 \% (Loc 1). 1,8-cineole was the major oil constituent (37.9 \%) of M. spicata plants from Loc 1. The major constituent of the inflorescence oils was piperitenone oxide which ranged from 32.4 \% (Loc 3) to 46.3 \% of the oil (Loc 1). The major constituent of the leaf oils was 1,8-cineole (40.5 \%) in plants from Loc 1 and trans-piperitone oxide in plants from Loc 2 (19.8 \%) and Loc 3 (33.5 \%). This is the first report for wild populations in Greece of a M. spicata oil in which 1,8-cineole is the major constituent. The observed variation in essential oil composition between locations and plant organs in July would not appear to be directly related to the climatic conditions.},
	pages = {225--230},
	number = {3},
	journaltitle = {Journal of Essential Oil Research},
	author = {Cook, Catherine M. and Kokkini, Stella and Lanaras, Thomas},
	urldate = {2013-08-28},
	date = {2007}
}

