	langid = {english},
	pmid = {23104886},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/MII2W8IA/Dobin et al. - 2013 - STAR ultrafast universal RNA-seq aligner.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/7SQIHNRJ/15.html:text/html}
