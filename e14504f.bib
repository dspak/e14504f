@article{kroken_phylogenomic_2003,
	title = {Phylogenomic analysis of type I polyketide synthase genes in pathogenic and saprobic ascomycetes},
	volume = {100},
	url = {http://www.pnas.org/content/100/26/15670.abstract},
	doi = {10.1073/pnas.2532165100},
	abstract = {Fungal type I polyketides ({PKs}) are synthesized by {PK} synthases ({PKSs}) and include well known secondary metabolites such as the anticholesterol drug lovastatin and the potent natural carcinogen aflatoxin. Other type I {PKs} are known to be virulence factors for some plant pathogens and pigments such as melanin. In this study, a phylogenomic approach was used to investigate the origin and diversity of fungal genes encoding putative {PKSs} that are predicted to synthesize type I {PKs}. The resulting genealogy, constructed by using the highly conserved {PKS} ketosynthase ({KS}) domain, indicated that: () Species within subphylum Pezizomycotina (phylum Ascomycota) but not early diverging ascomycetes, like (Saccharomycotina) or (Taphrinomycotina), had large numbers (7–25) of genes. () Bacteria and fungi had separate groups of genes; the few exceptions are the likely result of horizontal gene transfer from bacteria to various sublineages of fungi. () The bulk of genes encoding fungal {PKSs} fell into eight groups. Four groups were predicted to synthesize variously reduced {PKs}, and four groups were predicted to make unreduced {PKs}. () Species within different classes of Pezizomycotina shared the same groups of genes. () Different fungal genomes shared few putative orthologous genes, even between closely related genomes in the same class or genus. () The discontinuous distributions of orthologous s among fungal species can be explained by gene duplication, divergence, and gene loss; horizontal gene transfer among fungi does not need to be invoked.},
	pages = {15670--15675},
	number = {26},
	journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Kroken, Scott and Glass, N. Louise and Taylor, John W. and Yoder, O. C. and Turgeon, B. Gillian},
	urldate = {2009-02-05},
	date = {2003-12-23},
	file = {PNAS-2003-Kroken-15670-5.pdf:/Users/danielspakowicz/Zotero/storage/7PPK5AID/PNAS-2003-Kroken-15670-5.pdf:application/pdf;PNAS Full Text PDF:/Users/danielspakowicz/Zotero/storage/RWW8TBNM/Kroken et al - 2003 - Phylogenomic analysis of type I polyketide synthas.pdf:application/pdf;PNAS Snapshot:/Users/danielspakowicz/Zotero/storage/ETBT9QTT/15670.html:text/html;PNAS Snapshot:/Users/danielspakowicz/Zotero/storage/VUKJH3XD/15670.html:text/html}
}

