	urldate = {2016-12-21},
	date = {2010-01-05},
	keywords = {Asthma, Microbiome, Chronic obstructive pulmonary disease, Bacterial pathogens, Nose, Sequence databases, Ribosomal {RNA}, Infectious disease control},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/TRHRQGSV/Hilty et al. - 2010 - Disordered Microbial Communities in Asthmatic Airw.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/DTCRUGHE/article.html:text/html}
