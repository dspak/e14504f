@article{chanda_key_2009,
	title = {A key role for vesicles in fungal secondary metabolism},
	volume = {106},
	url = {http://www.pnas.org/content/106/46/19533.abstract},
	doi = {10.1073/pnas.0907416106},
	abstract = {Eukaryotes have evolved highly conserved vesicle transport machinery to deliver proteins to the vacuole. In this study we show that the filamentous fungus employs this delivery system to perform new cellular functions, the synthesis, compartmentalization, and export of aflatoxin; this secondary metabolite is one of the most potent naturally occurring carcinogens known. Here we show that a highly pure vesicle-vacuole fraction isolated from under aflatoxin-inducing conditions converts sterigmatocystin, a late intermediate in aflatoxin synthesis, to aflatoxin B; these organelles also compartmentalize aflatoxin. The role of vesicles in aflatoxin biosynthesis and export was confirmed by blocking vesicle-vacuole fusion using 2 independent approaches. Disruption of (encodes a protein homolog of {AvaA}, a small {GTPase} known to regulate vesicle fusion in ) or treatment with Sortin3 (blocks Vps16 function, one protein in the class C tethering complex) increased aflatoxin synthesis and export but did not affect aflatoxin gene expression, demonstrating that vesicles and not vacuoles are primarily involved in toxin synthesis and export. We also observed that development of aflatoxigenic vesicles (aflatoxisomes) is strongly enhanced under aflatoxin-inducing growth conditions. Coordination of aflatoxisome development with aflatoxin gene expression is at least in part mediated by ({VeA}), a global regulator of secondary metabolism. We propose a unique 2-branch model to illustrate the proposed role for {VeA} in regulation of aflatoxisome development and aflatoxin gene expression.},
	pages = {19533--19538},
	number = {46},
	journaltitle = {Proceedings of the National Academy of Sciences},
	author = {Chanda, Anindya and Roze, Ludmila V. and Kang, Suil and Artymovich, Katherine A. and Hicks, Glenn R. and Raikhel, Natasha V. and Calvo, Ana M. and Linz, John E.},
	urldate = {2009-11-18},
	date = {2009-11-17},
	file = {PNAS Full Text PDF:/Users/danielspakowicz/Zotero/storage/GJGK5XNI/Chanda et al - 2009 - A key role for vesicles in fungal secondary metabo.pdf:application/pdf;PNAS Snapshot:/Users/danielspakowicz/Zotero/storage/C7PWDUEW/19533.html:text/html}
}

