	urldate = {2013-04-04},
	date = {1998-06},
	file = {ScienceDirect Full Text PDF:/Users/danielspakowicz/Zotero/storage/P4AKEAPK/Peláez et al. - 1998 - Endophytic fungi from plants living on gypsum soil.pdf:application/pdf;ScienceDirect Snapshot:/Users/danielspakowicz/Zotero/storage/47MST2NB/S0953756208609442.html:text/html}
