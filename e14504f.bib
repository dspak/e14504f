@online{jae-hyuk_yu_regulation_2005,
	title = {Regulation of Secondary Metabolism in Filamentous Fungi},
	rights = {Copyright c 2005 by Annual Reviews. All rights reserved},
	url = {http://arjournals.annualreviews.org/doi/full/10.1146/annurev.phyto.43.040204.140214},
	type = {review-article},
