@article{kim-fuchs_prospective_2012,
	title = {Prospective randomized trial comparing sutured with sutureless mesh fixation for Lichtenstein hernia repair: long-term results},
	volume = {16},
	issn = {1248-9204},
	doi = {10.1007/s10029-011-0856-3},
	shorttitle = {Prospective randomized trial comparing sutured with sutureless mesh fixation for Lichtenstein hernia repair},
	abstract = {{BACKGROUND}

Following Lichtenstein hernia repair, up to 25\% of patients experience prolonged postoperative and chronic pain as well as discomfort in the groin. One of the underlying causes of these complaints are the compression or irritation of nerves by the sutures used to fixate the mesh. We compared the level and rate of chronic pain in patients operated with the classical Lichtenstein technique fixated by sutures to patients with sutureless mesh fixation technique.


{METHODS}

A two-armed randomized trial with 264 male patients was performed. After consent, patients were randomized preoperatively. For the fixation of the mesh we used either sutures with slow-absorbing material ({PDS} 2.0) (group I, n = 133) or tissue glue (Histoacryl) (group {II}, n = 131). Follow-up examinations were performed after 3, 12 months and after 5 years.


{RESULTS}

Patient characteristics in the two groups were similar. No cross-over between groups was observed. After 5 years, long-term follow-up could be completed for 59\% of subjects. After 5 years, 10/85 (11.7\%) patients in group I and 3/70 (4.2\%) in group {II} suffered from chronic pain in the groin region (P = 0.108). The operation time was significantly shorter in group {II} (79 min vs 73 min, P = 0.01). One early recurrence occurred in group {II} (3 months). The recurrence rate was 0 and 0\% after 12 months and 5.9\% (5/85) and 10\% (7/70) after 5 years in group I and group {II}, respectively (P = 0.379).


{CONCLUSION}

After 5 years, the two techniques of mesh fixation resulted in similar rates of chronic pain. Whereas recurrence rates were comparable, fixation of the mesh with tissue glue decreased operating room time significantly. Hence, suture less mesh fixation with Histoacryl is a sensible alternative to suture fixation and should be especially considered for patients prone to pain.},
	pages = {21--27},
	number = {1},
	journaltitle = {Hernia: the journal of hernias and abdominal wall surgery},
	shortjournal = {Hernia},
	author = {Kim-Fuchs, C and Angst, E and Vorburger, S and Helbling, C and Candinas, D and Schlumpf, R},
	date = {2012-02},
	pmid = {21789654},
	keywords = {Adult, Humans, Male, Time Factors, Aged, Aged, 80 and over, Chronic Pain, Enbucrilate, Follow-Up Studies, Hernia, Inguinal, Herniorrhaphy, Hypesthesia, Kaplan-Meier Estimate, Middle Aged, Pain, Postoperative, Recurrence, Suture Techniques, Sutures, Tissue Adhesives}
}

@article{wu_dynamics_1995,
	title = {Dynamics of lactose permease of Escherichia coli determined by site-directed chemical labeling and fluorescence spectroscopy},
	volume = {34},
	url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=7599118},
	abstract = {Mutants with a single Cys residue in place of Phe27, Pro28, Phe29, Phe30, or Pro31 at the periplasmic end of putative transmembrane helix I were used to study the interaction of lactose permease with ligand by site-directed chemical modification or fluorescence spectroscopy. With permease embedded in the native membrane, mutant Phe27--{\textgreater}Cys or Phe28--{\textgreater}Cys is readily labeled with [14C]-N-ethylmaleimide ({NEM}), while mutant Phe29--{\textgreater}Cys, Phe30--{\textgreater}Cys, or Phe31--{\textgreater}Cys reacts less effectively. beta,D-Galactopyranosyl 1-thio-beta,D-galactopyranoside ({TDG}) has little or no effect on the reactivity of Phe27--{\textgreater}Cys, Phe29--{\textgreater}Cys, or Phe30--{\textgreater}Cys permease. Remarkably, however, Pro31--{\textgreater}Cys permease which is essentially unreactive in the absence of ligand becomes highly reactive in the presence of {TDG}. Ligand also enhances the {NEM} reactivity of the mutant with Cys in place of Pro28 which is presumably on the same face of helix I as position 31. The five single-Cys mutants which also contain a biotin acceptor domain in the middle cytoplasmic loop were purified by monomeric avidin-affinity chromatography in dodecyl beta,D-maltoside and subjected to site-directed fluorescence spectroscopy. Mutants Phe27--{\textgreater}Cys, Phe29--{\textgreater}Cys, and Phe30--{\textgreater}Cys react rapidly with 2-(4'-maleimidylanilino)naphthalene-6-sulfonic acid ({MIANS}), and reactivity is not altered in the presence of {TDG}. In striking contrast, mutants Pro28--{\textgreater}Cys and Pro31--{\textgreater}Cys react extremely slowly with {MIANS} in the absent of ligand, and {TDG} dramatically enhances reactivity.({ABSTRACT} {TRUNCATED} {AT} 250 {WORDS})},
	pages = {8257--63},
	number = {26},
	journaltitle = {Biochemistry},
	author = {Wu, J. and Frillingos, S. and Kaback, H.R.},
	date = {1995-07-04}
}

