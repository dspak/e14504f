@article{choquer_ku70_2008,
	title = {\textit{Ku70} or {\textless}i{\textgreater}Ku80{\textless}/i{\textgreater} deficiencies in the fungus {\textless}i{\textgreater}Botrytis cinerea{\textless}/i{\textgreater} facilitate targeting of genes that are hard to knock out in a wild-type context},
