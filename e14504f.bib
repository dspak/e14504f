	shortjournal = {J. Am. Chem. Soc.},
	author = {Gu, Liangcai and Wang, Bo and Kulkarni, Amol and Gehret, Jennifer J and Lloyd, Kayla R and Gerwick, Lena and Gerwick, William H and Wipf, Peter and Håkansson, Kristina and Smith, Janet L and Sherman, David H},
	date = {2009-11-11},
