@article{gehret_terminal_2011,
	title = {Terminal Alkene Formation by the Thioesterase of Curacin A Biosynthesis {STRUCTURE} {OF} A {DECARBOXYLATING} {THIOESTERASE}},
	volume = {286},
	issn = {0021-9258, 1083-351X},
	url = {http://www.jbc.org/content/286/16/14445},
	doi = {10.1074/jbc.M110.214635},
	abstract = {Curacin A is a polyketide synthase ({PKS})-non-ribosomal peptide synthetase-derived natural product with potent anticancer properties generated by the marine cyanobacterium Lyngbya majuscula. Type I modular {PKS} assembly lines typically employ a thioesterase ({TE}) domain to off-load carboxylic acid or macrolactone products from an adjacent acyl carrier protein ({ACP}) domain. In a striking departure from this scheme the curacin A {PKS} employs tandem sulfotransferase and {TE} domains to form a terminal alkene moiety. Sulfotransferase sulfonation of β-hydroxy-acyl-{ACP} is followed by {TE} hydrolysis, decarboxylation, and sulfate elimination (Gu, L., Wang, B., Kulkarni, A., Gehret, J. J., Lloyd, K. R., Gerwick, L., Gerwick, W. H., Wipf, P., Håkansson, K., Smith, J. L., and Sherman, D. H. (2009) J. Am. Chem. Soc. 131, 16033–16035). With low sequence identity to other {PKS} {TEs} ({\textless}15\%), the curacin {TE} represents a new thioesterase subfamily. The 1.7-Å curacin {TE} crystal structure reveals how the familiar α/β-hydrolase architecture is adapted to specificity for β-sulfated substrates. A Ser-His-Glu catalytic triad is centered in an open active site cleft between the core domain and a lid subdomain. Unlike {TEs} from other {PKSs}, the lid is fixed in an open conformation on one side by dimer contacts of a protruding helix and on the other side by an arginine anchor from the lid into the core. Adjacent to the catalytic triad, another arginine residue is positioned to recognize the substrate β-sulfate group. The essential features of the curacin {TE} are conserved in sequences of five other putative bacterial {ACP}-{ST}-{TE} tridomains. Formation of a sulfate leaving group as a biosynthetic strategy to facilitate acyl chain decarboxylation is of potential value as a route to hydrocarbon biofuels.},
	pages = {14445--14454},
	number = {16},
	journaltitle = {Journal of Biological Chemistry},
	shortjournal = {J. Biol. Chem.},
	author = {Gehret, Jennifer J. and Gu, Liangcai and Gerwick, William H. and Wipf, Peter and Sherman, David H. and Smith, Janet L.},
	urldate = {2013-08-16},
	date = {2011-04-22},
	langid = {english},
	pmid = {21357626},
	keywords = {Crystal Structure, Decarboxylase, Enzyme Structure, Natural Products Biosynthesis, Protein Structure, Thioesterase, X-ray crystallography, polyketide synthase}
}

@article{hara_identification_1993,
	title = {Identification of Ras farnesyltransferase inhibitors by microbial screening},
	volume = {90},
	issn = {0027-8424},
	abstract = {A microbial screen using a yeast strain with conditional deficiency in the {GPA}1 gene was carried out to search for inhibitors of protein farnesyltransferase ({PFT}). A strain of Streptomyces was found to produce active compounds named {UCF}1-A, {UCF}1-B, and {UCF}1-C. Structural determination of these compounds revealed that {UCF}1-C is identical to the known antibiotic, manumycin, whereas {UCF}1-A and {UCF}1-B are structurally related to manumycin. All three {UCF}1 compounds suppress the lethality of gpa1 disruption, with {UCF}1-C exhibiting the strongest activity. {UCF}1 inhibits yeast as well as rat brain {PFT}. Fifty percent inhibition of yeast {PFT} activity is observed with 5 {microM} {UCF}1-C. Kinetic analyses of the inhibition suggest that {UCF}1-C acts as a competitive inhibitor of {PFT} with respect to farnesyl pyrophosphate, exhibiting a Ki of 1.2 {microM}, whereas the same compound appears to act as a noncompetitive inhibitor of {PFT} with respect to the farnesyl acceptor, the Ras protein. {UCF}1-C shows significant activity to inhibit the growth of Ki-ras-transformed fibrosarcoma, raising the possibility of its use as an antitumor drug.},
	pages = {2281--2285},
	number = {6},
	journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
	shortjournal = {Proc. Natl. Acad. Sci. U.S.A.},
	author = {Hara, M and Akasaka, K and Akinaga, S and Okabe, M and Nakano, H and Gomez, R and Wood, D and Uh, M and Tamanoi, F},
	date = {1993-03-15},
	pmid = {8460134},
	keywords = {Amino Acid Sequence, Animals, Brain, Humans, Male, Mice, Molecular Sequence Data, Alkyl and Aryl Transferases, Anti-Bacterial Agents, Antibiotics, Antineoplastic, Cattle, Cell Transformation, Neoplastic, Fibrosarcoma, Genes, Fungal, Genes, Lethal, Genes, ras, Glutathione Transferase, Mice, Inbred {BALB} C, Mice, Nude, Molecular Structure, Neoplasm Transplantation, Polyenes, Polyunsaturated Alkamides, Rats, Recombinant Fusion Proteins, Saccharomyces cerevisiae, Streptomyces, Structure-Activity Relationship, Transferases, Transplantation, Heterologous, Tumor Cells, Cultured}
