	keywords = {Recombinant Proteins, Aspergillus, Biocatalysis, Catalytic Domain, Cloning, Molecular, Fungal Proteins, Ketones, Lactones, Lovastatin, Malonyl Coenzyme A, Molecular Structure, Multienzyme Complexes, {NAD}, Naphthalenes, Polyketide Synthases, Pyrones, S-Adenosylmethionine, Saccharomyces cerevisiae, Substrate Specificity}
}

@article{foster_gutbrain_2013,
	title = {Gut–brain axis: how the microbiome influences anxiety and depression},
	volume = {36},
