@article{edgar_muscle:_2004,
	title = {{MUSCLE}: multiple sequence alignment with high accuracy and high throughput},
	volume = {32},
	url = {http://nar.oxfordjournals.org/cgi/content/abstract/32/5/1792},
	doi = {10.1093/nar/gkh340},
	shorttitle = {{MUSCLE}},
	abstract = {We describe {MUSCLE}, a new computer program for creating multiple alignments of protein sequences. Elements of the algorithm include fast distance estimation using kmer counting, progressive alignment using a new profile function we call the log-expectation score, and refinement using tree-dependent restricted partitioning. The speed and accuracy of {MUSCLE} are compared with T-Coffee, {MAFFT} and {CLUSTALW} on four test sets of reference alignments: {BAliBASE}, {SABmark}, {SMART} and a new benchmark, {PREFAB}. {MUSCLE} achieves the highest, or joint highest, rank in accuracy on each of these sets. Without refinement, {MUSCLE} achieves average accuracy statistically indistinguishable from T-Coffee and {MAFFT}, and is the fastest of the tested methods for large numbers of sequences, aligning 5000 sequences of average length 350 in 7 min on a current desktop computer. The {MUSCLE} program, source code and {PREFAB} test data are freely available at http://www.drive5. com/muscle.},
	pages = {1792--1797},
	number = {5},
	journaltitle = {Nucl. Acids Res.},
	author = {Edgar, Robert C.},
	urldate = {2008-12-04},
	date = {2004-03-19},
	file = {HighWire Full Text PDF:/Users/danielspakowicz/Zotero/storage/9EP69ESX/Edgar - 2004 - MUSCLE multiple sequence alignment with high accu.pdf:application/pdf;HighWire Snapshot:/Users/danielspakowicz/Zotero/storage/PNKEZPXU/1792.html:text/html}
}

@article{agger_diversity_2009,
	title = {Diversity of sesquiterpene synthases in the basidiomycete Coprinus cinereus},
	volume = {72},
	issn = {1365-2958},
	doi = {10.1111/j.1365-2958.2009.06717.x},
	abstract = {Fungi are a rich source of bioactive secondary metabolites, and mushroom-forming fungi (Agaricomycetes) are especially known for the synthesis of numerous bioactive and often cytotoxic sesquiterpenoid secondary metabolites. Compared with the large number of sesquiterpene synthases identified in plants, less than a handful of unique sesquiterpene synthases have been described from fungi. Here we describe the functional characterization of six sesquiterpene synthases (Cop1 to Cop6) and two terpene-oxidizing cytochrome P450 monooxygenases (Cox1 and Cox2) from Coprinus cinereus. The genes were cloned and, except for cop5, functionally expressed in Escherichia coli and/or Saccharomyces cerevisiae. Cop1 and Cop2 each synthesize germacrene A as the major product. Cop3 was identified as an alpha-muurolene synthase, an enzyme that has not been described previously, while Cop4 synthesizes delta-cadinene as its major product. Cop6 was originally annotated as a trichodiene synthase homologue but instead was found to catalyse the highly specific synthesis of alpha-cuprenene. Coexpression of cop6 and the two monooxygenase genes next to it yields oxygenated alpha-cuprenene derivatives, including cuparophenol, suggesting that these genes encode the enzymes for the biosynthesis of antimicrobial quinone sesquiterpenoids (known as lagopodins) that were previously isolated from C. cinereus and other Coprinus species.},
	pages = {1181--1195},
	number = {5},
	journaltitle = {Molecular Microbiology},
	shortjournal = {Mol. Microbiol.},
	author = {Agger, Sean and Lopez-Gallego, Fernando and Schmidt-Dannert, Claudia},
	date = {2009-06},
	pmid = {19400802},
	pmcid = {PMC2723806},
	keywords = {Phylogeny, Alkyl and Aryl Transferases, Cloning, Molecular, Coprinus, Cytochrome P-450 Enzyme System, Escherichia coli, Fungal Proteins, Genes, Fungal, Saccharomyces cerevisiae, Sesquiterpenes}
}

