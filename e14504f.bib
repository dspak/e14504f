@article{schoch_class-wide_2009,
	title = {A Class-Wide Phylogenetic Assessment of Dothideomycetes},
	volume = {64},
	issn = {0166-0616, 1872-9797},
	url = {http://www.studiesinmycology.org/content/64/1/1},
	doi = {10.3114/sim.2009.64.01},
	abstract = {We present a comprehensive phylogeny derived from 5 genes, {nucSSU}, {nucLSU} {rDNA}, {TEF}1, {RPB}1 and {RPB}2, for 356 isolates and 41 families (six newly described in this volume) in Dothideomycetes. All currently accepted orders in the class are represented for the first time in addition to numerous previously unplaced lineages. Subclass Pleosporomycetidae is expanded to include the aquatic order Jahnulales. An ancestral reconstruction of basic nutritional modes supports numerous transitions from saprobic life histories to plant associated and lichenised modes and a transition from terrestrial to aquatic habitats are confirmed. Finally, a genomic comparison of 6 dothideomycete genomes with other fungi finds a high level of unique protein associated with the class, supporting its delineation as a separate taxon.},
