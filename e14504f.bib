@article{rozowsky_peakseq_2009,
	title = {{PeakSeq} enables systematic scoring of {ChIP}-seq experiments relative to controls},
	volume = {27},
	rights = {© 2009 Nature Publishing Group},
	issn = {1087-0156},
	url = {http://www.nature.com/nbt/journal/v27/n1/full/nbt.1518.html},
	doi = {10.1038/nbt.1518},
	abstract = {Chromatin immunoprecipitation ({ChIP}) followed by tag sequencing ({ChIP}-seq) using high-throughput next-generation instrumentation is fast, replacing chromatin immunoprecipitation followed by genome tiling array analysis ({ChIP}-chip) as the preferred approach for mapping of sites of transcription-factor binding and chromatin modification. Using two deeply sequenced data sets for human {RNA} polymerase {II} and {STAT}1, each with matching input-{DNA} controls, we describe a general scoring approach to address unique challenges in {ChIP}-seq data analysis. Our approach is based on the observation that sites of potential binding are strongly correlated with signal peaks in the control, likely revealing features of open chromatin. We develop a two-pass strategy called {PeakSeq} to compensate for this. A two-pass strategy compensates for signal caused by open chromatin, as revealed by inclusion of the controls. The first pass identifies putative binding sites and compensates for genomic variation in the 'mappability' of sequences. The second pass filters out sites not significantly enriched compared to the normalized control, computing precise enrichments and significances. Our scoring procedure enables us to optimize experimental design by estimating the depth of sequencing required for a desired level of coverage and demonstrating that more than two replicates provides only a marginal gain in information.},
	pages = {66--75},
	number = {1},
	journaltitle = {Nature Biotechnology},
	shortjournal = {Nat Biotech},
	author = {Rozowsky, Joel and Euskirchen, Ghia and Auerbach, Raymond K. and Zhang, Zhengdong D. and Gibson, Theodore and Bjornson, Robert and Carriero, Nicholas and Snyder, Michael and Gerstein, Mark B.},
	urldate = {2016-02-28},
	date = {2009-01},
	langid = {english},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/Z4EHFU5R/Rozowsky et al. - 2009 - PeakSeq enables systematic scoring of ChIP-seq exp.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/R6B9SURR/nbt.1518.html:text/html}
}

@article{edgar_search_2010-1,
