@article{choi_improving_2014,
	title = {Improving polyketide and fatty acid synthesis by engineering of the yeast acetyl-{CoA} carboxylase},
	volume = {187},
	issn = {0168-1656},
	abstract = {Polyketides and fatty acids are important in the production of pharmaceuticals, industrial chemicals, and biofuels. The synthesis of the malonyl-{CoA} building block, catalyzed by acetyl-{CoA} carboxylase (Acc1), is considered a limiting step to achieving high titers of polyketides and fatty acids in Saccharomyces cerevisiae. Acc1 is deactivated by {AMP}-activated serine/threonine protein kinase (Snf1) when glucose is depleted. To prevent this deactivation, the enzyme was aligned with the Rattus norvegicus (rat) Acc1 to identify a critical amino acid (Ser-1157) for phosphorylation and deactivation. Introduction of a S1157A mutation into Acc1 resulted in 9-fold higher specific activity following glucose depletion. The enzyme was tested in yeast engineered to produce the polyketide 6-methylsalisylic acid (6-{MSA}). Both 6-{MSA} and native fatty acid levels increased by 3-fold. Utilization of this modified Acc1 enzyme will also be beneficial for other products built from malonyl-{CoA}. (C) 2014 Elsevier B.V. All rights reserved.},
	pages = {56--59},
	journaltitle = {Journal of Biotechnology},
	shortjournal = {J. Biotechnol.},
	author = {Choi, Jin Wook and Da Silva, Nancy A.},
	date = {2014-10},
	note = {{CCC}:000343837000008},
	keywords = {Saccharomyces cerevisiae, Acc1, Phosphorylation, 6-{MSA}, Polyketides,   Fatty acids, activated protein-kinase, saccharomyces-cerevisiae, lovastatin   biosynthesis, 6-methylsalicylic acid, phosphorylation sites, synthase,   snf1, substrate}
}

