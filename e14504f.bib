	keywords = {Adult, Female, Humans, Male, Metagenome, Mouth, Sequence Analysis, {DNA}, Young Adult, Adolescent, Aged, Aged, 80 and over, Cohort Studies, Middle Aged, Prospective Studies, Respiratory System, lung, Bronchoalveolar Lavage Fluid, Reference Values, Smoking}
