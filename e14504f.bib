	author = {Consortium, The Human Microbiome Project},
	urldate = {2014-09-25},
	date = {2012-06-14},
	langid = {english},
	keywords = {Genomics, Ecology, Health and medicine, Microbiology, genetics},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/B5HJB3CU/Consortium - 2012 - Structure, function and diversity of the healthy h.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/XIUC88M9/nature11234.html:text/html}
}

