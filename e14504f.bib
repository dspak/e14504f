	file = {SpringerLink Full Text PDF:/Users/danielspakowicz/Zotero/storage/4FAH2TX6/Michielse et al - 2005 - Agrobacterium-mediated transformation as a tool fo.pdf:application/pdf;SpringerLink Snapshot:/Users/danielspakowicz/Zotero/storage/PTC3NH39/u165412227821475.html:text/html}
}

