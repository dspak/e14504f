	author = {Consortium, The Human Microbiome Project},
	urldate = {2015-02-03},
	date = {2012-06-14},
	langid = {english},
	keywords = {Genomics, Ecology, Health and medicine, Microbiology, genetics},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/B5B8FIK6/Consortium - 2012 - Structure, function and diversity of the healthy h.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/6D4INIXT/nature11234.html:text/html}
