	pmid = {17526522},
	keywords = {Animals, Computational Biology, Genome, Humans, Internet, Artificial Intelligence, Automation, Chromosome Mapping, Database Management Systems, Documentation, Information Storage and Retrieval, Proteome, Signal Transduction, Vocabulary, Controlled, sequence analysis}
}

