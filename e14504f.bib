	date = {2010-03-04},
	pmid = {20203603},
	pmcid = {PMC3779803},
	keywords = {Adult, Bacteria, Gastrointestinal Tract, Genome, Bacterial, Genomics, Humans, Inflammatory Bowel Diseases, Metagenome, Sequence Analysis, {DNA}, Cohort Studies, Contig Mapping, Denmark, Feces, Genes, Bacterial, Genes, Essential, Health, Open Reading Frames, Overweight, Spain, obesity}
