@incollection{beth_saturation_2005,
	title = {Saturation Transfer {EPR}},
	url = {http://dx.doi.org/10.1007/0-306-48533-8_12},
	abstract = {Saturation transfer {EPR} ({ST}-{EPR}) has been utilized to characterize the very slow rotational diffusion of a wide range of membrane proteins over the past three decades. Development of computational algorithms for analyzing the {ST}-{EPR} data from membrane proteins undergoinguniaxial rotational diffusion and inclusion of these algorithms into global nonlinear least squares fitting routines has permitted detailed analyses of experimental data. These advances, coupled with access to multifrequency measurements, has established {ST}-{EPR} as an important complement to transient optical anisotropy for unraveling the complex rotational dynamics of intrinsic membrane proteins in cell membranes.},
