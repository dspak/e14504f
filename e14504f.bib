	number = {3},
	journaltitle = {The American Journal of Medicine},
	shortjournal = {The American Journal of Medicine},
	author = {Buchwald, Henry and Estok, Rhonda and Fahrbach, Kyle and Banel, Deirdre and Jensen, Michael D. and Pories, Walter J. and Bantle, John P. and Sledge, Isabella},
	urldate = {2016-07-29},
	date = {2009-03-01},
	keywords = {Bariatric surgery, Biliopancreatic diversion/duodenal switch, Gastric bypass, Gastroplasty, Laparoscopic adjustable gastric banding, Meta-analysis, Type 2 diabetes},
