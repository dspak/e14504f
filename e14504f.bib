@article{franzosa_identifying_2015,
	title = {Identifying personal microbiomes using metagenomic codes},
	volume = {112},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/112/22/E2930},
	doi = {10.1073/pnas.1423854112},
	abstract = {Community composition within the human microbiome varies across individuals, but it remains unknown if this variation is sufficient to uniquely identify individuals within large populations or stable enough to identify them over time. We investigated this by developing a hitting set-based coding algorithm and applying it to the Human Microbiome Project population. Our approach defined body site-specific metagenomic codes: sets of microbial taxa or genes prioritized to uniquely and stably identify individuals. Codes capturing strain variation in clade-specific marker genes were able to distinguish among 100s of individuals at an initial sampling time point. In comparisons with follow-up samples collected 30–300 d later, ∼30\% of individuals could still be uniquely pinpointed using metagenomic codes from a typical body site; coincidental (false positive) matches were rare. Codes based on the gut microbiome were exceptionally stable and pinpointed {\textgreater}80\% of individuals. The failure of a code to match its owner at a later time point was largely explained by the loss of specific microbial strains (at current limits of detection) and was only weakly associated with the length of the sampling interval. In addition to highlighting patterns of temporal variation in the ecology of the human microbiome, this work demonstrates the feasibility of microbiome-based identifiability—a result with important ethical implications for microbiome study design. The datasets and code used in this work are available for download from huttenhower.sph.harvard.edu/idability.},
	pages = {E2930--E2938},
	number = {22},
	journaltitle = {Proceedings of the National Academy of Sciences},
	shortjournal = {{PNAS}},
	author = {Franzosa, Eric A. and Huang, Katherine and Meadow, James F. and Gevers, Dirk and Lemon, Katherine P. and Bohannan, Brendan J. M. and Huttenhower, Curtis},
	urldate = {2016-05-09},
	date = {2015-06-02},
	langid = {english},
	pmid = {25964341},
	keywords = {Metagenomics, Microbial Ecology, forensic genetics, human microbiome, strain variation},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/ETN9TIN4/Franzosa et al. - 2015 - Identifying personal microbiomes using metagenomic.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/Q4JHED6X/E2930.html:text/html}
