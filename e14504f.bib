@incollection{merino_progress_2007,
	title = {Progress and Challenges in Enzyme Development for Biomass Utilization},
	url = {http://dx.doi.org/10.1007/10_2007_066},
	abstract = {Enzymes play a critical role in the conversion of lignocellulosic waste into fuels and chemicals, but the high cost of these enzymes presents a significant barrier to commercialization. In the simplest terms, the cost is a function of the large amount of enzyme protein required to break down polymeric sugars in cellulose and hemicellulose to fermentable monomers. In the past 6 years, significant effort has been expended to reduce the cost by focusing on improving the efficiency of known enzymes, identification of new, more active enzymes, creating enzyme mixes optimized for selected pretreated substrates, and minimization of enzyme production costs. Here we describe advances in enzyme technology for use in the production of biofuels and the challenges that remain.},
	pages = {95},
	booktitle = {Biofuels},
	author = {Merino, S. and Cherry, J.},
	date = {2007}
}

@article{menzella_combinatorial_2005,
	title = {Combinatorial polyketide biosynthesis by de novo design and rearrangement of modular polyketide synthase genes},
	volume = {23},
	rights = {© 2005 Nature Publishing Group},
	issn = {1087-0156},
	url = {http://www.nature.com/nbt/journal/v23/n9/full/nbt1128.html},
	doi = {10.1038/nbt1128},
	abstract = {Type I polyketide synthase ({PKS}) genes consist of modules 3–6 kb long, which encode the structures of 2-carbon units in polyketide products. Alteration or replacement of individual {PKS} modules can lead to the biosynthesis of 'unnatural' natural products but existing techniques for this are time consuming. Here we describe a generic approach to the design of synthetic {PKS} genes where facile cassette assembly and interchange of modules and domains are facilitated by a repeated set of flanking restriction sites. To test the feasibility of this approach, we synthesized 14 modules from eight {PKS} clusters and associated them in 154 bimodular combinations spanning over 1.5-million bp of novel {PKS} gene sequences. Nearly half the combinations successfully mediated the biosynthesis of a polyketide in Escherichia coli, and all individual modules participated in productive bimodular combinations. This work provides a truly combinatorial approach for the production of polyketides.},
	pages = {1171--1176},
	number = {9},
	journaltitle = {Nature Biotechnology},
	shortjournal = {Nat Biotech},
	author = {Menzella, Hugo G. and Reid, Ralph and Carney, John R. and Chandran, Sunil S. and Reisinger, Sarah J. and Patel, Kedar G. and Hopwood, David A. and Santi, Daniel V.},
