	number = {6},
	journaltitle = {Nature Biotechnology},
	shortjournal = {Nat Biotech},
	author = {Berlin, Konstantin and Koren, Sergey and Chin, Chen-Shan and Drake, James P. and Landolin, Jane M. and Phillippy, Adam M.},
	urldate = {2015-07-01},
	date = {2015-06},
	langid = {english},
	file = {Full Text PDF:/Users/danielspakowicz/Zotero/storage/U25DGJHS/Berlin et al. - 2015 - Assembling large genomes with single-molecule sequ.pdf:application/pdf;Snapshot:/Users/danielspakowicz/Zotero/storage/WG9G3R6S/nbt.3238.html:text/html}
}

