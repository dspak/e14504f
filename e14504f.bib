@article{nowobilski_lichtenstein_2004,
	title = {Lichtenstein inguinal hernioplasty using butyl-2-cyanoacrylate versus sutures. Preliminary experience of a prospective randomized trial},
	volume = {36},
	issn = {0014-312X},
	doi = {10.1159/000081646},
	abstract = {The Lichtenstein hernioplasty has become a popular method in inguinal hernia repair. This study compared two methods of mesh fixation and wound closure. Forty-six men with unilateral inguinal hernia were randomized into two groups. In the control group polypropylene mesh was anchored with 3/0 Dexon sutures, fascia and skin were closed with sutures 3/0 Dexon and 3/0 Monosof. In the study group, the mesh was secured with butyl-2-cyanoacrylate adhesive and the fascia and skin were also glued with the adhesive. The costs of materials, duration of the operation, amount of postoperative analgesic doses, pain score after the first and the 7th postoperative day and return to daily activity were recorded. No recurrences during the mean follow-up of 4.7 months were observed and the cosmetic effect was very good. In the study group with tissue adhesive the patients had significantly lower pain score after the first postoperative day and had a tendency to require less analgetic doses and to return earlier to their daily activity. Duration of the operation was similar in both groups. The cost of sutures and tissue adhesive used in both procedures was comparable. The use of tissue adhesive in mesh fixation and wound closure seems to be a promising technique in Lichtenstein hernia repair.},
	pages = {367--370},
	number = {6},
	journaltitle = {European surgical research. Europäische chirurgische Forschung. Recherches chirurgicales européennes},
	shortjournal = {Eur Surg Res},
	author = {Nowobilski, W and Dobosz, M and Wojciechowicz, T and Mionskowska, Lucjanna},
	date = {2004-12},
	pmid = {15591746},
	keywords = {Adult, Humans, Male, Aged, Biocompatible Materials, Enbucrilate, Hernia, Inguinal, Middle Aged, Polyglycolic Acid, Prospective Studies, Surgical Mesh, Suture Techniques, Sutures, Tissue Adhesives}
